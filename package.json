{
    "name": "frisklog",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "dev": "razzle start",
        "build": "razzle build",
        "test": "razzle test --env=jsdom",
        "start": "NODE_ENV=production node build/server.js",
        "precommit": "lint-staged",
        "format": "prettier --write '**/*.{js,jsx,css,md}'",
        "lint": "eslint . --cache --fix",
        "deploy": "gcloud app deploy && gcloud app browse"
    },
    "dependencies": {
        "@apollo/client": "^3.2.1",
        "@apollo/react-components": "^4.0.0",
        "@loadable/component": "^5.13.2",
        "@loadable/server": "^5.13.2",
        "@toast-ui/editor-plugin-code-syntax-highlight": "^1.0.0",
        "@toast-ui/react-editor": "^2.4.0",
        "axios": "^0.20.0",
        "bootstrap": "^5.1.3",
        "cookie-parser": "^1.4.5",
        "express": "^4.17.1",
        "github-markdown-css": "^4.0.0",
        "graphql": "^15.3.0",
        "highlight.js": "^10.2.1",
        "isomorphic-unfetch": "^3.1.0",
        "js-cookie": "^2.2.1",
        "jsonwebtoken": "^8.5.1",
        "marked": "^4.0.17",
        "moment": "^2.29.0",
        "react": "^17.0.1",
        "react-bootstrap": "^2.4.0",
        "react-dom": "^17.0.1",
        "react-helmet": "^6.1.0",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-slick": "^0.29.0",
        "slick-carousel": "^1.8.1",
        "styled-components": "^5.2.0",
        "styled-normalize": "^8.0.7",
        "react-loader-spinner": "^3.1.14"
    },
    "devDependencies": {
        "@loadable/babel-plugin": "^5.13.2",
        "@loadable/webpack-plugin": "^5.13.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.10.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.21.2",
        "eslint-plugin-standard": "^4.0.1",
        "lint-staged": "^13.0.3",
        "prettier": "^2.1.2",
        "razzle": "canary",
        "razzle-plugin-scss": "^4.2.17"
    },
    "lint-staged": {
        "*.{js,jsx,css,md}": [
            "prettier --write",
            "git add"
        ],
        "*.{js,jsx}": [
            "eslint --fix",
            "git add",
            "jest --findRelatedTests"
        ]
    }
}
